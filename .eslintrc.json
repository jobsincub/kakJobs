{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", {
      "trailingComma": "es5",
      "semi": false,
      "singleQuote": true,
      "endOfLine": "auto",
      "printWidth": 100,
      "arrowParens": "avoid"
    }]
  }
}
